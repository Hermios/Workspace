		{
			_mapWebsitesCodes.put(webSiteRoot, RandomStringU)
		}
		String pictureFullUrl=PathConstructor.getURL(webSiteRoot,pictureUrl);
		String picturePath=PathConstructor.getPath(webSiteId,pictureUrl);
		return "";
	}	
}
